<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wintertrain">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DialogActivity" />
        <activity android:name=".LearnForAcyivity" />
        <activity android:name=".LuckyActivity" />
        <activity android:name=".Fragment.LearnFranmentActivity" />
        <activity android:name=".NewPageerActivity" />
        <activity android:name=".PermissionActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".RecycleViewActivity"></activity>
    </application>

</manifest>